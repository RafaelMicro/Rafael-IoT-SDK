{
  // common setting
  "editor.formatOnSave": true,
  "C_Cpp.clang_format_path": "D:/rafael_tool/llvm-mingw-x86_64/bin/clang-format.exe",
  "C_Cpp.formatting": "clangFormat",
  // Cortex-Debug common
  "cortex-debug.openocdPath.windows": "${workspaceFolder}/tools/Debugger/OpenOCD/bin/win/openocd.exe",
  "cortex-debug.openocdPath.osx": "${workspaceFolder}/tools/Debugger/OpenOCD/bin/osx/openocd",
  "showDevDebugOutput": "raw",
  // Debug for RT584
  "cmake.debugConfig.RT58x": {
    "MIMode": "gdb",
    "name": "Cortex Debug (RT58x)",
    "executable": "${command:cmake.launchTargetPath}",
    "request": "launch",
    "type": "cortex-debug",
    "runToEntryPoint": "main",
    "servertype": "openocd",
    "configFiles": [
      "${workspaceFolder}/tools/Debugger/OpenOCD/script/interface/cmsis-dap.cfg",
      "${workspaceFolder}/tools/Debugger/OpenOCD/script/target/@MY_CHIP@.cfg"
    ],
    "searchDir": [
      "${workspaceFolder}/tools/Debugger/OpenOCD/script"
    ]
  },
  // Debug for RT584
  "cmake.debugConfig.RT584": {
    "MIMode": "gdb",
    "name": "Cortex Debug (RT584)",
    "executable": "${command:cmake.launchTargetPath}",
    "request": "launch",
    "type": "cortex-debug",
    "runToEntryPoint": "main",
    "servertype": "openocd",
    "configFiles": [
      "${workspaceFolder}/tools/Debugger/OpenOCD/script/interface/cmsis-dap.cfg",
      "${workspaceFolder}/tools/Debugger/OpenOCD/script/target/@MY_CHIP@.cfg"
    ],
    "searchDir": [
      "${workspaceFolder}/tools/Debugger/OpenOCD/script"
    ]
  },
  "files.associations": {
    "cstdint": "c",
    "mcu.h": "c",
    "chip_define.h": "c",
    "qspi.h": "c",
    "stdint.h": "c",
    "status.h": "c",
    "bit": "c",
    "rt_ecjpake.h": "c",
    "random": "c",
    "arm_cmse.h": "c",
    "bod_comp.h": "c",
    "rt_crypto.h": "c",
    "rt_sha256.h": "c",
    "rt_sm3.h": "c",
    "irm.h": "c",
    "lpm.h": "c",
    "aux_comp.h": "c",
    "sysctrl.h": "c"
  }
}
