            "searchDir": [
                "${workspaceFolder}/Tools/Debugger/OpenOCD/script",
                "${workspaceFolder}/Tools/Debugger/OpenOCD/target"
            ],
            "runToEntryPoint": "main",
            "showDevDebugOutput": "none",
            "svdFile": "${workspaceRoot}/Tools/Debugger/rt58x.svd"
        }
    ]
}
