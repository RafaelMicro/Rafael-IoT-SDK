CONFIG_SPI_LOOPBACK=y

#
# PROJECT CONFIG
#
CONFIG_APPLICATION_VERSION="1.0.0"

#
# PROJECT CONFIG
#
# end of PROJECT CONFIG
# end of PROJECT CONFIG

#
# SELECT DEVICE
#
# CONFIG_RT581 is not set
# CONFIG_RT582 is not set
CONFIG_RT583=y
